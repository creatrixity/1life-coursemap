  "version": "0.2.0",
  "compounds": [
    {
      "name": "Launch & Debug",
      "configurations": ["Launch NPM", "Launch Chrome"]
    }
  ],
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch NPM",
      "program": "${workspaceFolder}\\app\\server.js"
    },

    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
